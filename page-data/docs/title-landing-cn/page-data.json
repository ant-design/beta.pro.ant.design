md"}}},{"node":{"frontmatter":{"title":"内置布局","order":10,"type":"页面开发","time":null},"fields":{"slug":"/docs/layout-cn","path":"/docs/layout.zh-CN.md"}}},{"node":{"frontmatter":{"title":"开发前准备","order":9,"type":"入门","time":null},"fields":{"slug":"/docs/basic-knowledge-cn","path":"/docs/basic-knowledge.zh-CN.md"}}},{"node":{"frontmatter":{"title":"介绍","order":4,"type":"入门","time":null},"fields":{"slug":"/docs/introduction-cn","path":"/docs/introduction.zh-CN.md"}}},{"node":{"frontmatter":{"title":"开始使用","order":3,"type":"入门","time":null},"fields":{"slug":"/docs/getting-started-cn","path":"/docs/getting-started.zh-CN.md"}}},{"node":{"frontmatter":{"title":"文档总览","order":0,"type":"入门","time":null},"fields":{"slug":"/docs/overview-cn","path":"/docs/overview.zh-CN.md"}}}]}},"pageContext":{"slug":"/docs/title-landing-cn","type":"/docs/","locale":"/-cn/"}},"staticQueryHashes":[]}